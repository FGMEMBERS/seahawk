<?xml version="1.0"?>

<!-- emergency flap loering -->

<PropertyList>

 <path>switch-hook.ac</path>

<!-- **************** handle **************** -->
<animation>
  <type>rotate</type>
  <object-name>cylinder</object-name>
  <property>controls/gear/tailhook</property>
  <interpolation>
   <entry><ind>0</ind><dep>-30</dep></entry>
   <entry><ind>1</ind><dep>30</dep></entry>
  </interpolation>
  <center>
   <x>0.01</x>
   <y>0</y>
   <z>0</z>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>
 
 </PropertyList>



